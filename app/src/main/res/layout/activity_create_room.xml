<?xml version="1.0" encoding="utf-8"?>
<android.widget.FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#17EDE5D1"
    tools:context=".create_room">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TableRow
                android:layout_width="96dp"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="37dp"
                    android:layout_marginTop="256dp"
                    android:background="#FFB300"
                    android:foregroundGravity="center"
                    android:gravity="center"
                    android:text="방이름" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <EditText
                    android:id="@+id/cname"
                    android:layout_width="284dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="256dp"
                    android:background="#23FFB300"
                    android:textColor="#000000"
                    android:textSize="13sp" />
            </TableRow>
        </TableRow>
        <TableRow
            android:layout_width="190dp"
            android:layout_height="match_parent">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="128dp"
                    android:layout_height="39dp"
                    android:background="#FFB300"
                    android:gravity="center"
                    android:text="연락처" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <EditText
                    android:id="@+id/cnumber"
                    android:layout_width="285dp"
                    android:layout_height="match_parent"
                    android:background="#23FFB300"
                    android:textColor="#000000"
                    android:textSize="13sp" />
            </TableRow>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="43dp">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFB300"
                    android:gravity="center"
                    android:text="위도" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/cwee"
                    android:layout_width="285dp"
                    android:layout_height="41dp"
                    android:background="#23FFB300"
                    android:textColor="#000000"
                    android:textSize="13sp" />
            </TableRow>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="128dp"
                    android:layout_height="40dp"
                    android:background="#FFB300"
                    android:gravity="center"
                    android:text="경도" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/cgyeong"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#23FFB300"
                    android:textColor="#000000"
                    android:textSize="13sp" />
            </TableRow>
        </TableRow>
        <Button
            android:id="@+id/btn2"
            android:layout_width="151dp"
            android:layout_height="41dp"
            android:layout_marginTop="60dp"
            android:text="위도경도 받아오기"
            app:backgroundTint="#FFB300"
            app:layout_constraintVertical_bias="0.521" />
        <Button
            android:id="@+id/btn1"
            android:layout_width="285dp"
            android:layout_height="41dp"
            android:text="방만들기"
            app:backgroundTint="#FFB300"
            app:layout_constraintVertical_bias="0.521" />
    </TableLayout>
</android.widget.FrameLayout>
